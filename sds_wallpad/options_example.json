{
	"serial_mode": "serial",
	"entrance_mode": "off",
	"wallpad_mode": "on",
	"intercom_mode": "off",
	"serial": {
		"port":  "/dev/ttyUSB0",
		"baudrate": 9600,
		"bytesize": 8,
		"parity": "E",
		"stopbits": 1
	},
	"socket": {
		"address": "192.168.1.1",
		"port": 8899
	},
	"mqtt": {
		"discovery": true,
		"prefix": "sds",
		"server": "127.0.0.1",
		"port": 1883,
		"need_login": false,
		"user": "",
		"passwd": ""
	},
	"rs485": {
		"max_retry": 20,
		"early_response": 2,
		"dump_time": 0
	},
	"log": {
		"to_file": true,
		"filename": "/share/sds_wallpad.log"
	}
}
